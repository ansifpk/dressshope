    <!-- Main JS File -->
     <script>
       function copyCode() {
        var couponElement = document.getElementById(`code`)
        var tempInput = document.createElement("input");
        tempInput.value = `${'<%=baseUrl%>'}/register?refferalCode=${couponElement.textContent}`;
        document.body.appendChild(tempInput);
        tempInput.select();
        tempInput.setSelectionRange(0, 99999);
        document.execCommand("copy");
        document.body.removeChild(tempInput);
        
        Swal.fire({
         text: "Reffaral Url copied",
        });
        
        }
     </script>
