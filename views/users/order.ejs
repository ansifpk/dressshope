    <script>
       
        function changePage(status) {
            let currentPage = document.getElementById("currentPage").innerText*1
            const totalPage = document.getElementById("totalPage").innerText*1
           
            let limit = 4;
           if(status == "increment" && currentPage<totalPage){
             currentPage++
            }else if(status == "decrement" && currentPage>1){
               currentPage--
           }else{
            return;
           }
           document.getElementById("currentPage").innerText=`${currentPage}`;
             fetch(`/paginationOrder?page=${currentPage}`,{method:"GET"}).then((res)=>res.json())
            .then((res)=>{
               
                 if(res.orderData.length == 0){
                      document.getElementById("table").innerHTML= `
                       <tr>
                        <td>
                            No orders
                        </td>
                       </tr>
                      `
                 }else{
                   document.getElementById("table").innerHTML=  res.orderData.map((order,index)=>{
                    let price = order.products.reduce((acc,pro)=>{
                        if(pro.productStatus == "Delivered" || pro.productStatus == "returnPending" || pro.productStatus == "pending" ){
                            return acc+pro.productTotal
                        }else{
                            return acc;
                        }   
                    },0);
                    if(price !== 0){
                        price = price - order.couponOfferPrice;
                    }
                    let count = limit *( currentPage-1)
                        
               return `
                    <tr>
                        <td class="product-col text-center">
                           ${count+index+1}
                        </td>
                        <td class="text-center" >${order.products[0].deliveryAddress.address}</td>
                        <td class="text-center">${price}</td>
                        <td class="text-center">${new Date(order.createdAt).toLocaleDateString()}</td>
                        <td class="text-center">
                            <a href="/orderDetailes?_id=${order._id}" type="button" class="btn btn-outline-primary-2">
                            <span>View Detailes</span>
                            <i class="icon-long-arrow-right"></i>
                            </a>
                        </td>                
                    </tr>
                `
                }).join("")
            }     
          })
       }    
    
