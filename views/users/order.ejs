                                        <table class="table table-cart table-mobile">
									<thead>
										<tr>
											<th class="text-center">SI No</th>
											<th class="text-center">Delivery Address</th>
											<th class="text-center">Total</th>
											<th class="text-center">Date</th>
											<th class="text-center">Action</th>
										</tr>
									</thead>

									<tbody id="table">
                                        <%
                                        if(orderData.length > 0){
                                            orderData.forEach((order,index) => {
                                                    let total = order.products.reduce((acc,pro)=>{
                                                      if(pro.productStatus == "Delivered" || pro.productStatus == "returnPending" || pro.productStatus == "pending" ){
                                                        return acc+pro.productTotal
                                                      }else{
                                                        return acc;
                                                      }
                                                    },0)
                                                    if(total !== 0){
                                                        total = total - order.couponOfferPrice;
                                                    }
                                                    %>
                                                       <tr>
                                                            <td class="product-col text-center">
                                                                <%=index+1%>
                                                            </td>
                                                            <td class="text-center" ><%=order.products[0].deliveryAddress.address%></td>
                                                            <td class="text-center"><%=total%></td>
                                                            <td class="text-center"><%=new Date(order.createdAt).toLocaleDateString()%></td>
                                                            <td class="flex" >
                                                                <a href="/orderDetailes?_id=<%=order._id%>" type="button" class="btn btn-outline-primary-2">
                                                                <span>View Detailes</span>
                                                                <i class="icon-long-arrow-right"></i>
                                                                </a>
                                                            </td>  
                                                       </tr>
                                                    <%
                                             });         
                                        }else{
                                            %>
                                              <tr>
                                                <td>No Orders</td>
                                              </tr>
                                            <%
                                        }
                                        %>
									</tbody>
								</table><!-- End .table table-wishlist -->
                                            
								    </div><!-- .End .tab-pane -->
								</div>
