                                                                 <%
                                                                  

                                                                if(orderData.products[0].productStatus==='Delivered' || orderData.products[0].paymentMethod !== "COD"){
                                                                    let showDiscount = false;
                                                                    orderData.products.forEach((cur)=>{
                                                                        if(cur.productStatus == "pending" || cur.productStatus == "Delivered" || cur.productStatus == "returnPending" ){
                                                                          showDiscount = true;
                                                                          
                                                                        }
                                                                    })
                                                                    %>
                                                                     <a id="invoiceButton" class="btn btn-primary mt-1 text-white <%=showDiscount?"":"disabled"%>"  onclick="DownloadInvoice(this,'<%=orderData._id%>')"><span>Download invoice</span></a>
                                                                    <%
                                                                }
                                                                 %> 
                                                                 <%
                                                                 let pending = false;
                                                                 let delivered = false;
                                                                 let failed = false;
                                                                 orderData.products.forEach((product)=>{
                                                                         if(product.productStatus == "pending"){
                                                                            pending = true;
                                                                         }else if(product.productStatus == "Failed"){
                                                                            failed = true;
                                                                         }else if(product.productStatus == "Delivered"){
                                                                            delivered = true;
                                                                         }
                                                                 })
                                                                 %>
                                                               
                                                                  <%
                                                                  if (pending) {%>
                                                                     <a id="actionButton" onclick="handleAllCancell(this,'<%=orderData._id%>')" class="btn btn-primary mt-1 text-white" ><span>Cancell All</span></a>
                                                                  <%} else if(failed) {%>
                                                                     <a id="actionButton" class="btn btn-primary mt-1 text-white" onclick="handleAllPayagain(this,'<%=orderData._id%>')" ><span>Pay Again</span></a>
                                                                  <%}else if(delivered){%>
                                                                     <a id="actionButton" class="btn btn-primary mt-1 text-white" onclick="handleAllReturn(this,'<%=orderData._id%>')" ><span>Return All</span></a>
                                                                  <%}
                                                                  %>
                                                                 
                                                            </div>
