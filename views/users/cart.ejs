    <!-- End .modal -->

    <button id="scroll-top" title="Back to Top">
      <i class="icon-arrow-up"></i>
    </button>
    <script>
      function updateCart(tag, productId) {
        const previousValue = tag.getAttribute("data-prev");
        if (tag.value == tag.max) {
          Swal.fire("Maximum quandity is exceeds!");
        }

        fetch(`/updateCart`, {
          method: "PATCH",
          body: JSON.stringify({ productId, quandity: tag.value }),
          headers: { "Content-Type": "application/json" },
        })
          .then((res) => res.json())
          .then((data) => {
            if (data.success) {
              console.log(data)
              document.getElementById(
                `productTotal${productId}`
              ).innerText = `Rs.${data.productTotal}.00`;
              document.getElementById(
                "SubTotal"
              ).innerText = `Rs.${data.subTotal}.00`;
              document.getElementById(
                "total"
              ).innerText = `Rs.${data.total}.00`;
              let discount = document.getElementById(
                "discount"
              )
                discount.innerText = discount.innerText.replace("Rs -","")
                discount.innerText = discount.innerText.replace(".00","")
                discount.innerText = `Rs ${data.discount}.00`;
              
              
            } else {
              tag.value = previousValue;
              Swal.fire({
                title: data.message,
                icon: "warning",
              });
            }
          });
      }

      function deleteCart(productId) {
        const productTotal = document.getElementById(`productTotal${productId}`).innerText.replace("Rs.","").replace(".00","");
        const swalWithBootstrapButtons = Swal.mixin({
          customClass: {
            confirmButton: "btn btn-success",
            cancelButton: "btn btn-danger",
          },
          buttonsStyling: false,
        });
        swalWithBootstrapButtons
          .fire({
            title: "Are you sure?",
            text: "You Want to remove this product!",
            icon: "warning",
            showCancelButton: true,
            confirmButtonText: "Yes, Removed it!",
            cancelButtonText: "No, cancel!",
            reverseButtons: true,
          })
          .then((result) => {
            if (result.isConfirmed) {
              

              fetch(`/deleteCart?productId=${productId}`)
                .then((res) => res.json())
                .then((data) => {
           
              swalWithBootstrapButtons.fire({
                title: "Removed!",
                text: "Iteam  has been Removed.",
                icon: "success",
              });
              const remove = document.getElementById(`remove${productId}`);
              remove.remove();
              const subTotal = document.getElementById("SubTotal");
              const total = document.getElementById("total");
              const discount = document.getElementById("discount");
              subTotal.innerText = `Rs.${data.subTotal}.00`;
              if(data.subTotal == 0){
                total.innerText = `Rs.00.00`;
                discount.innerText = `Rs.00.00`;
                document.getElementById("tableBody").innerHTML = `
                      <tr>
                        <td>No prodcuts in your cart</td>
                      </tr>
                      <tr>
                        <td>
                          <a
                            href="/products"
                            class="btn btn-outline-dark-2 btn-block mb-3"
                            ><span>CONTINUE SHOPPING</span
                            ><i class="icon-refresh"></i
                          ></a>
                        </td>
                      </tr>
                `
                document.getElementById("priceBar").remove()
              }
              if(!data.checkOffer) discount.innerText = `Rs.00.00`;
              document.getElementById("total").innerText = `Rs.${data.total}.00`;
              
              });
            } 
          });
      }
    </script>

    <script>
      function handleProceedCheckOut() {
        fetch("/proceedCheckout")
          .then((res) => res.json())
          .then((res) => {
            if (res.success) {
              return (window.location.href = "/checkout");
            } else {
              Swal.fire({
                title: res.message,
                icon: "warning",
              });
            }
          });
      }
    </script>
