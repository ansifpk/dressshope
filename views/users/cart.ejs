      <main class="main">
        <div
          class="page-header text-center"
          style="background-image: url('assets/images/page-header-bg.jpg')"
        >
          <div class="container">
            <h1 class="page-title">Shopping Cart<span>Shop</span></h1>
          </div>
          <!-- End .container -->
        </div>
        <!-- End .page-header -->
        <div class="page-content">
          <div class="cart">
            <div class="container">
              <div class="row">
                <div class="col-lg-9">
                  <table class="table table-cart table-mobile">
                    <thead>
                      <tr>
                        <th>Product</th>
                        <th>Price</th>
                        <th>Quantity</th>
                        <th>Total</th>
                        <th></th>
                      </tr>
                    </thead>

                    <tbody id="tableBody">
                      <% if(productData.products.length){%>
                      <%productData.products.forEach((product)=>{ const offer =
                      offerData?.find( offer => offer?.iteam ===
                      product.productId.name || offer?.iteam ===
                      product.productId.categoryID.name ); %>
                      <tr id="remove<%=product.productId._id%>">
                        <td class="product-col">
                          <div class="product">
                            <figure class="product-media">
                              <a href="/detailes?id=<%=product.productId._id%>">
                                <img
                                  src="<%=product.productId.image[0].secure_url%>"
                                  alt="Product image"
                                />
                              </a>
                            </figure>

                            <h3 class="product-title">
                              <a><%=product.productId.name%></a>
                            </h3>
                          </div>
                        </td>

                        <td>
                            <%- offer 
                                ? `<span class='new-price'>Rs.${product.productId.Price - Math.round(product.productId.Price * offer.offerRate / 100)}.00</span>
                                <span class='old-price'>Rs.${product.productId.Price}.00</span>`
                                : `Rs.${product.productId.Price}.00` 
                            %>
                        </td>

                        
                        <td class="quantity-col">
                          <div class="cart-product-quantity">
                            <input
                              type="number"
                              class="form-control"
                              id="quandity<%=product.productId._id%>"
                              value="<%=product.quandity%>"
                              min="1"
                              max="4"
                              onchange="updateCart(this,'<%=product.productId._id%>')"
                              step="1"
                              data-decimals="0"
                              required
                              onfocus="this.setAttribute('data-prev', this.value)"
                            />
                          </div>
                        </td>
                        <td id="productTotal<%=product.productId._id%>" class="total-col">
                          Rs.<%=offer ?
                          product.quandity * product.productId.Price - Math.round(product.productId.Price * offer.offerRate/100) 
                          : 
                          product.productId.Price * product.quandity %>.00
                        </td>
                        <td class="remove-col">
                          <button
                            class="btn-remove"
                            onclick="deleteCart('<%=product.productId._id%>')"
                          >
                            <i class="icon-close"></i>
                          </button>
                        </td>
                      </tr>
                      <%})%> <%}else{%>
                      <tr>
                        <td>No prodcuts in your cart</td>
                      </tr>
                      <tr>
                        <td>
                          <a
                            href="/products"
                            class="btn btn-outline-dark-2 btn-block mb-3"
                            ><span>CONTINUE SHOPPING</span
                            ><i class="icon-refresh"></i
                          ></a>
                        </td>
                      </tr>
                      <%} %>
                    </tbody>
                  </table>
                </div>
                <% if(productData.products.length){%>
                <aside id="priceBar" class="col-lg-3">
                  <div class="summary summary-cart">
                    <h3 class="summary-title">Cart Total</h3>
                   

                    <table class="table table-summary">
                      <tbody>
                        <%const price = productData.products.reduce((acc,cur)=>acc+(cur.productId.Price*cur.quandity),0)  %>
                        <tr class="summary-subtotal">
                          <td >Subtotal:</td>
                          <td id="SubTotal" >Rs.<%=price%>.00</td>
                        </tr>

                        <tr class="summary-shipping-row">
                          <td>
                            <div class="custom-control custom-radio">
                              <input
                                type="checkbox"
                                checked="true"
                                disabled
                                id="free-shipping"
                                name="shipping"
                                class="custom-control-input"
                              />
                              <label
                                class="custom-control-label"
                                for="free-shipping"
                                >Offer Price
                              </label>
                            </div> 
                          </td> 
                          <td class="text-danger" id="discount" >Rs <%=subTotal == 0 ? 8:subTotal-price%>.00</td>
                        </tr>
                        <tr class="summary-total">
                          <td>Total:</td>
                          <td id="total">Rs.<%=subTotal%>.00</td>
                        </tr>
                      </tbody>
                    </table>
                    <a
                      onclick="handleProceedCheckOut()" 
                      class="btn btn-outline-primary-2 btn-order btn-block"
                      >PROCEED TO CHECKOUT</a>
                  </div>

                  <a
                    href="/products"
                    class="btn btn-outline-dark-2 btn-block mb-3"
                    ><span>CONTINUE SHOPPING</span><i class="icon-refresh"></i></a>
                </aside>
                <%} %>
              </div>
              <!-- End .row -->
            </div>
            <!-- End .container -->
